{
  "manifest_version": 3,
  "name": "name",
  "version": "1.0.0",
  "description": "an utterly necessary extension to be created",
  "icons": {
    "16": "assets/icons/missing.png",
    "48": "assets/icons/missing.png",
    "128": "assets/icons/missing.png"
  },
  "action": {
    "default_icon": {
      "16": "assets/icons/missing.png",
      "48": "assets/icons/missing.png",
      "128": "assets/icons/missing.png"
    }
  },
  "permissions": [
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["injection.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["dist/main.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
